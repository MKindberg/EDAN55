package lab3;

import java.util.LinkedList;

public class IndSet {
	
	
	public static void main(String[] args) {
		LinkedList<LinkedList<Integer>> graph;
	}
	
	public int R0(LinkedList<LinkedList<Integer>> graph){
		if (graph.size()==0)
			return 0;
		for(LinkedList l:graph)
			if(l.size()==0){
				graph.remove(l);
				return 1+R0();
			}
		int max=0;
		LinkedList maxN=null;
		
		for(LinkedList l:graph)//get max neighbors
			if(l.size()>max){
				max = l.size();
				maxN = l;
			}
		return 
	}
}
